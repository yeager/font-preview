#!/usr/bin/env python3
import sys
import os
sys.path.insert(0, '/usr/lib/python3/dist-packages')

try:
    from font_preview.main import main
    main()
except ImportError:
    try:
        from font_preview.app import main
        main()
    except ImportError:
        print("Error: Cannot find main module for font-preview", file=sys.stderr)
        sys.exit(1)
